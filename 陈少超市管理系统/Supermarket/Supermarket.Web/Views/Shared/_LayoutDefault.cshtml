<!DOCTYPE html>
@{ 
    if (Session["Admin"] != null)
    {
        ViewBag.Name = (Session["Admin"] as Supermarket.EneityFramework.AdminInfo).UserName;
    }
    else
    {
        ViewBag.Name = "奇迹开发组";
    }
}
<html>
<head>
    <meta name="viewport" content="width=device-width" charset="UTF-8"/>
    <title>@ViewBag.Title</title>

    <!--引入样式-->
    <!-- Bootstrap样式  -->
    <link href="~/Content/Layout/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!-- 图标样式 -->
    <link href="~/Content/Layout/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- 主题样式 -->
    <link href="~/Content/Layout/dist/css/AdminLTE.min.css" rel="stylesheet" />
    <!-- 主题皮肤 Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link href="~/Content/Layout/dist/css/skins/_all-skins.min.css" rel="stylesheet" />
    <!--表格样式-->
    <link href="~/Content/Layout/bootstrap-table-develop/dist/bootstrap-table.min.css" rel="stylesheet" />
    <!--下拉框select样式-->
    <link href="~/Content/Layout/bootstrap-select-1.12.4/dist/css/bootstrap-select.min.css" rel="stylesheet" />

    <!--引入Js-->
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!-- 布局页Js -->
    <script src="~/Content/Layout/dist/js/adminlte.min.js"></script>
    <script src="~/Content/Layout/dist/js/demo.js" charset="gbk" ></script>
    <!--bootstrap-table.js以及启用编辑所需js-->
    <script src="~/Content/Layout/bootstrap-table-develop/dist/bootstrap-table.min.js"></script>
    <script src="~/Content/Layout/bootstrap-table-develop/dist/locale/bootstrap-table-zh-CN.js"></script>
    <script src="~/Content/Layout/bootstrap-select-1.12.4/js/bootstrap-select.js"></script>
    <script src="~/Content/script/My97DatePicker/WdatePicker.js"></script>
    <script src="~/Content/script/Extension.js"></script>
    <script src="~/Content/script/verification.js"></script>
    <script src="~/Content/Layout/Exit.js"></script>
    <script src="~/Content/Layout/Default.js"></script>
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <!-- Header -->
        <header class="main-header">
            <!-- Logo -->
            <a href="index2.html" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>Miracle</b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>欢迎来到超市管理系统</b></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>

                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Messages: style can be found in dropdown.less-->
                        <li class="dropdown messages-menu" id="all" title="点击全屏显示">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-life-ring"></i>
                            </a>
                        </li>
                        <!-- Notifications: style can be found in dropdown.less -->
                        <li class="dropdown notifications-menu" title="通知">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-bell-o"></i>
                                <span class="label label-warning">10</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="header">您有10个通知</li>
                                <li>
                                    <!-- inner menu: contains the actual data -->
                                    <ul class="menu">
                                        <li>
                                            <a href="#">
                                                <i class="fa fa-users text-aqua"></i> 今天新进了一批货物
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <i class="fa fa-warning text-yellow"></i> T-258号商品库存不足，请及时进货
                                                page and may cause design problems
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <i class="fa fa-users text-red"></i> 你更改了你的用户名
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <i class="fa fa-shopping-cart text-green"></i> F-146号需要进货
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <i class="fa fa-user text-red"></i> 欢迎登陆
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                                <li class="footer"><a href="#">查看全部</a></li>
                            </ul>
                        </li>
                        <!-- Tasks: style can be found in dropdown.less -->
                        <li class="dropdown tasks-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-flag-o"></i>
                                <span class="label label-danger">9</span>
                            </a>
                        </li>
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/Content/Layout/dist/img/admin1.jpg" class="user-image" alt="User Image">
                                <span class="hidden-xs">@ViewBag.Name</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="~/Content/Layout/dist/img/admin1.jpg" class="img-circle" alt="User Image">

                                    <p>
                                        @ViewBag.Name
                                        <small>July since Nov. 2017</small>
                                    </p>
                                </li>
                                <!-- Menu Body -->
                                <li class="user-body">
                                    <div class="row">
                                        <div class="col-xs-4 text-center">
                                            <a href="#">通知</a>
                                        </div>
                                        <div class="col-xs-4 text-center">
                                            <a href="#">邮件</a>
                                        </div>
                                        <div class="col-xs-4 text-center">
                                            <a href="#">个性设置</a>
                                        </div>
                                    </div>
                                    <!-- /.row -->
                                </li>
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="#" class="btn btn-default btn-flat">个人信息</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="#" class="btn btn-default btn-flat" id="Exit">退出</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                        <!-- Control Sidebar Toggle Button -->
                        <li>
                            <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="~/Content/Layout/dist/img/admin1.jpg" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p><a href="/Home/Index">@ViewBag.Name</a></p>
                        <a href="#"><i class="fa fa-circle text-success"></i> 在线</a>
                    </div>
                </div>
                <!-- search form -->
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" name="q" class="form-control" placeholder="搜索">
                        <span class="input-group-btn">
                            <button type="submit" name="search" id="search-btn" class="btn btn-flat">
                                <i class="fa fa-search"></i>
                            </button>
                        </span>
                    </div>
                </form>
                <!-- /.search form -->
                <!-- sidebar menu-->
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header">主要导航</li><!--active-->
                    <li class="treeview">
                        <a href="javascript:void(0);">
                            <i class="fa fa-user"></i> <span> 管理员维护 </span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li class="active"><a href="/AdminInfo/Index"><i class="fa fa-circle-o"></i> 查看管理员列表 </a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="javascript:void(0);">
                            <i class="fa fa-user-circle"></i>
                            <span> 会员管理 </span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/User/Index"><i class="fa fa-circle-o"></i> 查看会员列表 </a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="JavaScript:void(0);">
                            <i class="fa fa-calendar-times-o"></i>
                            <span>商品类别管理</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/Category/Index"><i class="fa fa-circle-o"></i> 查看类别列表</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="JavaScript:void(0);">
                            <i class="fa fa-product-hunt"></i>
                            <span>商品管理</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/Product/Index"><i class="fa fa-circle-o"></i> 查看商品列表</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="JavaScript:void(0);">
                            <i class="fa fa-laptop"></i>
                            <span>销售查询</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/Sales/Index"><i class="fa fa-circle-o"></i> 查看销售信息</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="javascript:void(0);">
                            <i class="fa fa-edit"></i> <span>库存管理</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/Stock/Index"><i class="fa fa-circle-o"></i> 查看库存信息</a></li>
                            <li><a href="/Purchases/Index"><i class="fa fa-circle-o"></i> 查看入库信息</a></li>
                            <li><a href="/ReturnGoods/Index"><i class="fa fa-circle-o"></i> 查看出库信息</a></li>
                        </ul>
                    </li>
                    <li class="treeview">
                        <a href="javascript:void(0);">
                            <i class="fa fa-cog"></i> <span>系统维护</span>
                            <span class="pull-right-container">
                                <i class="fa fa-angle-left pull-right"></i>
                            </span>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="/SysLog/Index"><i class="fa fa-circle-o"></i> 查看系统日志</a></li>
                        </ul>
                    </li>

                    <li>
                        <a href="javascript:void(0);">
                            <i class="fa fa-tags"></i> <span>其他应用</span>
                            <span class="pull-right-container">
                                <span class="label label-primary pull-right">4</span>
                            </span>
                        </a>
                    </li>
                    <li class="header">标签</li>
                    <li><a href="javascript:void(0);"><i class="fa fa-circle-o text-red"></i> <span>重要</span></a></li>
                    <li><a href="javascript:void(0);"><i class="fa fa-circle-o text-yellow"></i> <span>警告</span></a></li>
                    <li><a href="javascript:void(0);"><i class="fa fa-circle-o text-aqua"></i> <span>消息</span></a></li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    SuperMarket
                    <small>Control panel</small>
                </h1>
                <ol class="breadcrumb">
                    <li><a href="#"><i class="@ViewBag.MapIcon"></i>Home</a></li>
                    <li class="active">@ViewBag.Map1</li>
                    <li class="active">@ViewBag.Map2</li>

                </ol>
            </section>

            <!-- Main content -->
            <section class="content">
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.4.0
            </div>
            <strong>Copyright &copy; 2016-2017 <a href="#">奇迹开发组</a>.</strong> All rights
            reserved.
        </footer>
        <aside class="control-sidebar control-sidebar-dark">
            <div class="tab-content">
                <div class="tab-pane" id="control-sidebar-home-tab">

                </div>
            </div>
        </aside>
    </div>
</body>
</html>
