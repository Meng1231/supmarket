
@{
    ViewBag.Title = "出库信息";
    ViewBag.MapIcon = "fa fa-edit";
    ViewBag.Map1 = "库存管理";
    ViewBag.Map2 = "出库信息";
}
<script src="~/Content/script/ReturnGoods/ReturnGoods.js"></script>
<div>
    <form class="well">
        <label>商品名称</label> &nbsp;
        <input type="text" style="font-size:12px;height:25px;" id="ProductNmae" placeholder=" 请输入商品名称">
        <label style="margin-left:30px;">商品类别</label> &nbsp;
        <select name="RoleID" style="width:190px;" id="ProductType">
            <option value="0">请选择</option>
        </select>
        <label style="margin-left:20px;">出库时间</label> &nbsp;&nbsp;
        <input id="StarTime" class="Wdate" style="cursor:pointer" onfocus="WdatePicker()" />
        <label>至</label>
        <input id="EndTime" class="Wdate" style="cursor:pointer" onfocus="WdatePicker()" />
        <button type="button" style="margin-left:150px;" id="btn_query" class="btn btn-primary fa fa-search"> 查 询 </button>
    </form>
</div>
<table id="ReturnGoods" class="table"></table>
<div id="toolbar" class="btn-group">
    <button id="btn_add" type="button" class="btn btn-default">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span> 新 增
    </button>
    <button id="btn_edit" type="button" class="btn btn-default">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> 编 辑
    </button>
    <button id="btn_delete" type="button" class="btn btn-default">
        <span class="glyphicon glyphicon-remove" aria-hidden="true"></span> 删 除
    </button>
</div>

<form id="form" method="post">
    <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="margin-top:120px;">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">添加会员信息</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <label for="txt_rolename">出库数量</label>
                        <input type="text" name="ReturnAmount" class="form-control" id="txt_ReturnAmount" placeholder="数量">
                    </div>
                    <div class="form-group">
                        <label for="txt_roledesc">出库原因</label>
                        <input type="text" name="Remark" class="form-control" id="txt_Remark" placeholder="出库原因">
                    </div>
                    <div class="form-group hidden">
                        <label for="txt_defaulturl_Web">出库编号</label>
                        <input type="text" name="ReturnGoodsID" class="form-control" id="txt_ReturnGoodsID" placeholder="">
                    </div>
                    <div class="form-group">
                        <label style="font-size:12px;color:red;" id="lbl_error"></label>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default fa fa-close" data-dismiss="modal"> 关闭</button>
                    <button type="button" id="btn_submit" class="btn btn-primary fa fa-save"> 保存</button>
                </div>
            </div>
        </div>
    </div>
</form>

